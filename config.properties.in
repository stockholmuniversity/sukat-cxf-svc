# Jetty settings
cxf-server.http.port               = 80

# bind.address (Specify to bind to specific ip. Not mandatory)
cxf-server.bind.address            = 127.0.0.1
cxf-server.ssl.enabled             = false
cxf-server.ssl.keystore            = cxf-svc-server.keystore
cxf-server.ssl.password            = changeit

# Jetty spnego settings
cxf-server.spnego.conf             = /etc/spnego.conf
cxf-server.spnego.properties       = spnego.properties
cxf-server.spnego.realm            = SU.SE
cxf-server.spnego.kdc              = kerberos.su.se

# Sets the endpoint url in the wsdl, ex https://sukat-svc.it.su.se/
cxf-app.publishedEndpointUrl = %%ENDPOINT_URL%%

# ldap settings
cxf-app.ldap.serverro           = ldap://ldap-test.su.se
cxf-app.ldap.serverrw           = ldap://sukat-test-ldaprw02.it.su.se

